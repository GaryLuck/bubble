20 FOR I = 1 TO 1000
25 @(I) = 1000-I
48 PRINT @(I),
50 NEXT I
60 PRINT
70 PRINT "--------------------"
100 N=1000
110 FOR I = 1 TO N-1
120 FOR J = 1 TO N-1
130 IF @(J) < @(J+1) THEN 170
140 X = @(J)
150 @(J) = @(J+1)
160 @(J+1) =X
170 NEXT J
180 NEXT I
200 FOR I = 1000
210 PRINT @(I),
220 NEXT I
225 PRINT
230 END
